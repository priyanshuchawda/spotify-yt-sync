{
  "name": "spotify-yt-sync",
  "version": "1.0.0",
  "private": true,
  "description": "Sync Spotify liked songs into a YouTube playlist",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "export:spotify-liked": "node src/export-liked-songs.js",
    "export:spotify-liked:csv": "node src/export-liked-songs.js --format csv",
    "export:spotify-playlist": "node src/export-playlist-tracks.js",
    "export:spotify-playlist:csv": "node src/export-playlist-tracks.js --format csv"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.2"
  }
}
